{
  "private": true,
  "scripts": {
    "start": "yarn run live-server --port=8085 --quiet",
    "build:sw": "yarn build:sw:injectManifest && yarn build:sw:webpack",
    "build:sw:injectManifest": "yarn run workbox injectManifest workbox-config.js",
    "build:sw:webpack": "yarn run webpack --config webpack.sw.config.js",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "msw": "^0.29.0",
    "workbox-cacheable-response": "5.1.3",
    "workbox-core": "5.1.3",
    "workbox-expiration": "5.1.3",
    "workbox-precaching": "5.1.3",
    "workbox-routing": "5.1.3",
    "workbox-strategies": "5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@testing-library/dom": "^7.30.3",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/user-event": "^13.1.2",
    "@types/jest": "^26.0.21",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^4.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "live-server": "^1.2.1",
    "prettier": "^2.3.0",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "workbox-cli": "5.1.3"
  },
  "lint-staged": {
    "**/*.js": "yarn run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  }
}
